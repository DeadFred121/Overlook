<h1>Contact number:</h1>

<%= form_for :enquiry, :url => { :action => "enquiry" } do |f| %>
    <%= f.label :name %>
  <%= f.text_field :name %><br>
  <%= f.label :email %>
  <%= f.text_field :email %><br>
  <%= f.label :phone %>
  <%= f.text_field :phone %><br>
  <%= f.label :rooms %>
  <%= f.select :room, ["Classic Room", "Deluxe King Room", "Superior King Room", "Overlook Suite", "Presidential Cottage"] %><br>
  <%= f.label :message %>
  <%= f.text_area :message, :size => "60x12" %><br>
  <%= submit_tag "Create" %>
<% end %>

<p>Toll-Free: (800) 976-1377</p>
<p>Local: (970) 577-4000</p>
